{
    "python.envFile": "${workspaceFolder}/.venv/bin/python",
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "python.testing.pytestArgs": [
        "-s",
        "--capture=no"
    ],
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.typeCheckingMode": "off",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.autoSearchPaths": true,
    "python.analysis.extraPaths": [
        "src"
    ],
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "debug.javascript.terminalOptions": {},
    "debug.javascript.debugByLinkOptions": "on",
    "black-formatter.args": [
        "--line-length",
        "140"
    ],
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "evenBetterToml.formatter.allowedBlankLines": 2,
    "cSpell.words": [
        "Acceldata",
        "ACCOUNTADMIN",
        "addopts",
        "apikey",
        "argjson",
        "bizevent",
        "bizevents",
        "CISO",
        "cloudevent",
        "Compuware",
        "dashboarding",
        "DATEADD",
        "deadsnakes",
        "devel",
        "docstrings",
        "doesn't",
        "dsoa",
        "DTAGENT",
        "ECAC",
        "ensurepath",
        "externalbrowser",
        "getsizeof",
        "HISTCONTROL",
        "Huyen",
        "ignorespace",
        "indentless",
        "isna",
        "JDBC",
        "jdoe",
        "jsonstrip",
        "judgements",
        "Kamatchi",
        "libcairo",
        "libgdk",
        "libpango",
        "libpangocairo",
        "listagg",
        "Luhansk",
        "Majeure",
        "memoizable",
        "MILLIS",
        "misallocated",
        "Mitich",
        "Mizrahi",
        "multitenancy",
        "mysnowflake",
        "Neev",
        "pandoc",
        "pango",
        "parseable",
        "perc",
        "pipx",
        "pixbuf",
        "pylint",
        "pytest",
        "PYTHONPATH",
        "quiescing",
        "Ramdoss",
        "representer",
        "resattr",
        "Secoda",
        "SECURITYADMIN",
        "Shukla",
        "snowagent",
        "snowflakecomputing",
        "snowflakedb",
        "snowpark",
        "SNOWPIPE",
        "specversion",
        "spendings",
        "SQLERRM",
        "SQLROWCOUNT",
        "subrows",
        "subspan",
        "Suri",
        "timeadd",
        "timediff",
        "Trapelo",
        "tzlocal",
        "UNDROP",
        "unforseen",
        "UNREDACTED",
        "USECASES",
        "uuidgen",
        "venv",
        "Vidhya",
        "Waltham",
        "weasyprint",
        "Xometry"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/{info,lfs,logs,refs,objects}/**",
        ".git/{index,*refs,*HEAD}",
        ".vscode",
        ".vscode-insiders",
        "conf/config-*.json",
        "*.toml"
    ],
    "focus.highlightRange": "none",
    "pylint.path": ["${workspaceFolder}/.venv/bin/pylint"],
    "pylint.args": [
        "--disable=C0111,C0415,C0301,W0621", 
        "--max-line-length=140"
    ],
    "vsicons.presets.jsonOfficial": true,
    "vsicons.presets.yamlOfficial": true,
    "vsicons.projectDetection.autoReload": true,
    "gitlens.views.contributors.files.icon": "status",
    "workbench.iconTheme": "vscode-icons",
    "markdownlint.config": {
        "MD041": false,
        "MD033": false,
        "MD022": false,
        "MD028": false
    },
    "markdown.extension.toc.updateOnSave": true,
    "markdown.extension.toc.unorderedList.marker": "*",
    "markdown.extension.toc.levels": "2..6",
    "markdown.extension.toc.orderedList": false,
    "markdown.extension.toc.slugifyMode": "github",
    "markdown.extension.list.indentationSize": "inherit",
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[ignore]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "python.terminal.activateEnvInCurrentTerminal": true
}